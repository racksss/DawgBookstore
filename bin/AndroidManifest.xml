<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dawgbookstore"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.dawgbookstore.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.dawgbookstore.CourseListOne"
            android:label="@string/title_activity_course_list_one"
            android:parentActivityName="com.example.dawgbookstore.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dawgbookstore.MainActivity" />
        </activity>
        <activity
            android:name="com.example.dawgbookstore.BookListOne"
            android:label="@string/title_activity_book_list_one"
            android:parentActivityName="com.example.dawgbookstore.CourseListOne" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dawgbookstore.CourseListOne" />
        </activity>
        <activity
            android:name="com.example.dawgbookstore.BookOne"
            android:label="@string/title_activity_book_one"
            android:parentActivityName="com.example.dawgbookstore.BookListOne" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dawgbookstore.BookListOne" />
        </activity>
        <activity
            android:name="com.example.dawgbookstore.BookListTwo"
            android:label="@string/title_activity_book_list_two"
            android:parentActivityName="com.example.dawgbookstore.CourseListOne" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dawgbookstore.CourseListOne" />
        </activity>
        <activity
            android:name="com.example.dawgbookstore.SellBookPage"
            android:label="@string/title_activity_sell_book_page"
            android:parentActivityName="com.example.dawgbookstore.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dawgbookstore.MainActivity" />
        </activity>
        <activity
            android:name="com.example.dawgbookstore.MessageManager"
            android:label="@string/title_activity_message_manager"
            android:parentActivityName="com.example.dawgbookstore.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dawgbookstore.MainActivity" />
        </activity>
    </application>

</manifest>
